generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Library {
  id          String   @id @default(uuid())
  name        String
  description String
  latitude    Float
  longitude   Float
  photoUrl    String
  createdAt   DateTime @default(now())
}
